config system global
    set hostname "${faz2_vm_name}"
end

config system interface
    edit "port1"
        set ip ${faz2_ipaddr}/${faz_mask}
        set allowaccess ping https ssh
    next
end
config system route
    edit 1
        set device "port1"
        set gateway ${faz_gw}
    next
end

config system ha
 set mode a-p
 set group-id 10
 set group-name FAZAZURE
 set hb-interface "port1"
  config peer
   edit 1
    set serial-number ${faz1_byol_serial_number}
    set addr ${faz1_ipaddr}
   next
  end
 set preferred-role secondary
  config vip
   edit 1
    set vip ${ha_ip_address}
    set vip-interface port1
   next
  end
end

